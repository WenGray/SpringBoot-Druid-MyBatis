<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- User-->
<mapper namespace="com.spring.dao.UserDao">

   <select id="findAll" resultType="com.spring.entity.User">
      select
        user_id, name, age
      from
        user
   </select>

</mapper>
